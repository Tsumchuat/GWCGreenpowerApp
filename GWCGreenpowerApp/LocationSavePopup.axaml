<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="" d:DesignHeight="450"
        Width="450" Height="150"
        x:Class="GWCGreenpowerApp.LocationSavePopup"
        Title="LocationSavePopup">
        <StackPanel Margin="15" Spacing="10">
                <TextBlock Text="Enter preset name:"/>
                <TextBox x:Name="NameBox" Width="300"/>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="10">
                        <Button x:Name="CancelButton" Content="Cancel" Width="80"/>
                        <Button x:Name="OkButton" Content="Save" Width="80" IsDefault="True"/>
                </StackPanel>
        </StackPanel>
</Window>
